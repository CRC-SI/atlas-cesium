{
  "preset": "google",
  "excludeFiles": [
    "node_modules/**",
    "lib/**",
    "test/**"
  ],
  "validateQuoteMarks": {
    "escape": true,
    "mark": "'"
  },
  "disallowMixedSpacesAndTabs": true,
  "disallowMultipleLineStrings": true,
  "disallowSpacesInFunctionExpression": {
    "beforeOpeningRoundBrace": true
  },
  /* Overriding Google preset. */
  "maximumLineLength": 100,
   "requireCurlyBraces": [
    //"if",
    "else",
    "for",
    "while",
    "do",
    "try",
    "catch"
  ],
  "disallowKeywords": [
    "console"
  ]
}
