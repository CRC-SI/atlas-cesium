{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["./src"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(tests/*)|((^|\\/|\\\\)_)"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    },
    "opts": {
        "private": true,
        "recurse": true,
        "lenient": true,
        "destination": "jsdocs/"
    }
}
